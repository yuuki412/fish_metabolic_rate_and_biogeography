#R codes used in Watanabe and Payne (2022) Nat. Commun.
#Phylogenetic Mixed Model
#Prepare for tree using the software Mesquite in tree.nex format (here "34spp.tree.nex"))
#Prepare for data in csv format with headers of animal, RMR, mass, and temp (here "dataset.csv"))
#Species name in tree should match "animal"

library(MCMCglmm) #Call package
datos=read.csv("dataset.csv",header=TRUE) #Read data
tree=read.nexus("34spp.trees.nex") #Read tree
plot(tree) #Check tree
inv.phylo<-inverseA(tree,nodes="TIPS",scale=TRUE) #Create inverse matrix
prior<-list(G=list(G1=list(V=1,nu=0.02)),R=list(V=1,nu=0.02)) #Set prior
model01<-MCMCglmm(log10(RMR)~log10(mass)+temp,random=~animal,family="gaussian",ginverse=list(animal=inv.phylo$Ainv),prior=prior,data=datos,nitt=500000,burnin=1000,thin=500) #Run model
summary(model01) #Show result

#Data extraction from FishBase using rfishbase
#Extracting lifestyle of the species included in the metabolic rate datasets

library("rfishbase") #Call package
library("dplyr") #Call package

