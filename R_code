dd <- species(species_list(Order='Lamniformes')) %>% select(SpecCode, Species, Saltwater)
e <- stocks(species_list(Order='Carcharhiniformes')) %>% select(SpecCode, Species, Northernmost, NorthSouthN, Southermost, NorthSouthS, EnvTemp)
ee <- species(species_list(Order='Carcharhiniformes')) %>% select(SpecCode, Species, Saltwater)
f <- stocks(species_list(Order='Squaliformes')) %>% select(SpecCode, Species, Northernmost, NorthSouthN, Southermost, NorthSouthS, EnvTemp)
ff <- species(species_list(Order='Squaliformes')) %>% select(SpecCode, Species, Saltwater)
g <- stocks(species_list(Order='Echinorhiniformes')) %>% select(SpecCode, Species, Northernmost, NorthSouthN, Southermost, NorthSouthS, EnvTemp)
gg <- species(species_list(Order='Echinorhiniformes')) %>% select(SpecCode, Species, Saltwater)
h <- stocks(species_list(Order='Pristiophoriformes')) %>% select(SpecCode, Species, Northernmost, NorthSouthN, Southermost, NorthSouthS, EnvTemp)
hh <- species(species_list(Order='Pristiophoriformes')) %>% select(SpecCode, Species, Saltwater)
i <- stocks(species_list(Order='Squatiniformes')) %>% select(SpecCode, Species, Northernmost, NorthSouthN, Southermost, NorthSouthS, EnvTemp)
ii <- species(species_list(Order='Squatiniformes')) %>% select(SpecCode,
Species, Saltwater)
j <- stocks(species_list(Order='Torpediniformes')) %>% select(SpecCode, Species, Northernmost, NorthSouthN, Southermost, NorthSouthS, EnvTemp)
jj <- species(species_list(Order='Torpediniformes')) %>% select(SpecCode, Species, Saltwater)
k <- stocks(species_list(Order='Rhinopristiformes')) %>% select(SpecCode, Species, Northernmost, NorthSouthN, Southermost, NorthSouthS, EnvTemp)
kk <- species(species_list(Order='Rhinopristiformes')) %>% select(SpecCode, Species, Saltwater)
l <- stocks(species_list(Order='Rajiformes')) %>% select(SpecCode, Species, Northernmost, NorthSouthN, Southermost, NorthSouthS, EnvTemp)
ll <- species(species_list(Order='Rajiformes')) %>% select(SpecCode, Species, Saltwater)
m <- stocks(species_list(Order='Myliobatiformes')) %>% select(SpecCode, Species, Northernmost, NorthSouthN, Southermost, NorthSouthS, EnvTemp)
mm <- species(species_list(Order='Myliobatiformes')) %>% select(SpecCode, Species, Saltwater)

aaa <- merge(a,aa)
bbb <- merge(b,bb)
ccc <- merge(c,cc)
ddd <- merge(d,dd)
eee <- merge(e,ee)
fff <- merge(f,ff)
ggg <- merge(g,gg)
hhh <- merge(h,hh)
iii <- merge(i,ii)
jjj <- merge(j,jj)
kkk <- merge(k,kk)
lll <- merge(l,ll)
mmm <- merge(m,mm)

abc <- rbind(aaa,bbb,ccc,ddd,eee,fff,ggg,hhh,iii,jjj,kkk,lll,mmm)
write.csv(abc,"C:/Users/Watanabe/Desktop/abc.csv") #Write data on my desktop

