#Extracting latitudinal range, climate category, and habitat category (saltwater or not) for all extant species
#Elasmobranch
a <- stocks(species_list(Order='Hexanchiformes')) %>% select(SpecCode, Species, Northernmost, NorthSouthN, Southermost, NorthSouthS, EnvTemp)
aa <- species(species_list(Order='Hexanchiformes')) %>% select(SpecCode, Species, Saltwater)
b <- stocks(species_list(Order='Heterodontiformes')) %>% select(SpecCode, Species, Northernmost, NorthSouthN, Southermost, NorthSouthS, EnvTemp)
bb <- species(species_list(Order='Heterodontiformes')) %>% select(SpecCode, Species, Saltwater)
